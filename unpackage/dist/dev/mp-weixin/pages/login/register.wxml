<view class="padding-login"><view class="border-bottom"><view><text class="iconfont iconxingming color-green" style="font-size:26px;"></text><text class="text-margin">姓名</text></view><input placeholder="请输入姓名" name="input" data-event-opts="{{[['input',[['__set_model',['$0','account','$event',[]],['userInfo']]]]]}}" value="{{userInfo.account}}" bindinput="__e"/></view><view class="border-bottom"><view><text class="iconfont iconipad color-green" style="font-size:26px;"></text><text class="text-margin">手机</text></view><input placeholder="请输入手机号码" type="number" name="input" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['userInfo']]]]]}}" value="{{userInfo.phone}}" bindinput="__e"/></view><view class="border-bottom"><view><text class="iconfont iconsecurity color-green" style="font-size:26px;"></text><text class="text-margin">验证码</text></view><view class="cu-form-group"><input placeholder="请输入验证码" name="input" data-event-opts="{{[['input',[['__set_model',['$0','captcha','$event',[]],['userInfo']]]]]}}" value="{{userInfo.captcha}}" bindinput="__e"/><button class="cu-btn line-green shadow" disabled="{{disabled}}" data-event-opts="{{[['tap',[['codeClick',['$event']]]]]}}" bindtap="__e">{{btnTitle+txt}}</button></view></view><view class="border-bottom"><view><text class="iconfont iconpassword color-green" style="font-size:26px;"></text><text class="text-margin">密码</text></view><input placeholder="请设置密码" type="password" name="input" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['userInfo']]]]]}}" value="{{userInfo.password}}" bindinput="__e"/></view><view class="border-bottom"><view><text class="iconfont iconpassword color-green" style="font-size:26px;"></text><text class="text-margin">请确认密码</text></view><input placeholder="请确认密码" type="password" name="input" data-event-opts="{{[['input',[['__set_model',['$0','passwordComfirm','$event',[]],['userInfo']]]]]}}" value="{{userInfo.passwordComfirm}}" bindinput="__e"/></view><button data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="cu-btn block bg-green margin-tb-sm lg positon-btn" style="margin-top:100rpx;" bindtap="__e">注册</button></view>