page {
  height: 100%;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  font-size: 14px;
  color: #333333;
  overflow-x: hidden;
}
.f10 {
  font-size: 10px;
}
.f12 {
  font-size: 12px;
}
.f20 {
  font-size: 20px;
}
.cr {
  color: #00AE66;
}
.cr1 {
  color: #333;
}
.cr2 {
  color: #666;
}
.cr3 {
  color: #999999;
}
.clearfix {
  clear: both;
}
.workMethod .method-center {
  margin-top: 30rpx;
  background-color: #fff;
  padding: 30rpx;
}
.workMethod .method-center .imgIcon {
  width: 40rpx;
  height: 40rpx;
}
.workMethod .method-top {
  background-color: #fff;
  padding: 30rpx;
}
.workMethod .method-top .scroll-view_H {
  padding: 20rpx 0;
  height: 140rpx;
  white-space: nowrap;
  -webkit-box-sizing: content-box;
          box-sizing: content-box;
  width: calc(100vw - 60rpx);
}
.workMethod .method-top .scroll-view_H image {
  display: inline-block;
  width: 200rpx;
  height: 100%;
  margin-right: 10rpx;
  border-radius: 3px;
}
.workMethod label {
  display: block;
  font-size: 34rpx;
  border-bottom: 1px solid #E1E1E1;
  padding-bottom: 16rpx;
  margin-bottom: 20rpx;
}
.workMethod label .line {
  display: inline-block;
  width: 4px;
  height: 33rpx;
  background-color: #00AE66;
  margin-right: 8px;
  vertical-align: text-top;
}

